<h1>Welcome to the Advanced Dungeons and Dragons Character Creator</h1>
<%= link_to("Create a Character", new_character_path) %>
<h2>Characters</h2>
  <ul id="character">
    <% @character.each do |character| %>
      <li>
        <strong><%= link_to character.cname, character %></strong>
      </li>
    <% end %>
  </ul>
